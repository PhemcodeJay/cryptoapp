{
  "name": "cryptoapp",
  "version": "1.0.0",
  "description": "Web3-enabled portfolio tracker and trading bot with Node.js, Express, React, and blockchain integration",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "npm run dev:fullstack",
    "dev:server": "nodemon server.js",
    "dev:client": "cd client && npm run dev",
    "dev:fullstack": "concurrently -n \"BACKEND,FRONTEND\" -c \"bgGreen.bold,bgBlue.bold\" \"npm run dev:server\" \"npm run dev:client\"",
    "test": "jest --passWithNoTests",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PhemcodeJay/cryptoapp.git"
  },
  "keywords": [
    "crypto",
    "blockchain",
    "trading-bot",
    "web3",
    "defi"
  ],
  "author": "PhemcodeJay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PhemcodeJay/cryptoapp/issues"
  },
  "homepage": "https://github.com/PhemcodeJay/cryptoapp#readme",
  "dependencies": {
    "@web3-react/core": "^8.2.3",
    "@web3-react/injected-connector": "^6.0.7",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.6",
    "chart.js": "^4.4.9",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.4.1",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-router-dom": "^7.6.0",
    "sequelize": "^6.37.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.3",
    "supertest": "^6.3.4",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.19.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
